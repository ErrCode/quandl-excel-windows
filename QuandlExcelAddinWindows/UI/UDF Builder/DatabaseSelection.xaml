<UserControl x:Class="Quandl.Excel.Addin.UI.UDF_Builder.DatabaseSelection"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Quandl.Excel.Addin.UI.UDF_Builder"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Label x:Name="label" Content="Database Selection" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Center" Margin="0,180,0,0" TextWrapping="Wrap" Text="{Binding TestText}" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
